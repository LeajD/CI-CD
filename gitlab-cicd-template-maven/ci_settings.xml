<settings>
  <servers>
    <server>
      <id>gitlab-maven</id>
      <configuration>
        <httpHeaders>
          <property>
            <name>Private-Token</name> <!-- name must match gitlab token's type and not gitlab token's name -->
            <value>${PackageRegistryToken}</value> <!-- token -->
          </property>
        </httpHeaders>
      </configuration>
    </server>
  </servers>
</settings>
